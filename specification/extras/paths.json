{
  "/repositories": {
    "get": {
      "parameters": [
        {
          "enum": ["admin", "contributor", "member", "owner"],
          "in": "query",
          "name": "role",
          "required": false,
          "type": "string"
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/commits": {
    "get": {
      "parameters": [
        {
          "in": "query",
          "name": "include",
          "required": false,
          "type": "string"
        },
        {
          "in": "query",
          "name": "exclude",
          "required": false,
          "type": "string"
        }
      ]
    },
    "post": {
      "consumes": ["application/x-www-form-urlencoded"],
      "parameters": [
        {
          "in": "body",
          "name": "include",
          "required": false,
          "type": "string"
        },
        {
          "in": "body",
          "name": "exclude",
          "required": false,
          "type": "string"
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/commits/{revision}": {
    "get": {
      "parameters": [
        {
          "in": "query",
          "name": "include",
          "required": false,
          "type": "string"
        },
        {
          "in": "query",
          "name": "exclude",
          "required": false,
          "type": "string"
        }
      ]
    },
    "post": {
      "parameters": [
        {
          "in": "body",
          "name": "include",
          "required": false,
          "type": "string"
        },
        {
          "in": "body",
          "name": "exclude",
          "required": false,
          "type": "string"
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/deploy-keys": {
    "post": {
      "parameters": [
        {
          "in": "body",
          "name": "key",
          "required": true,
          "type": "string"
        },
        {
          "in": "body",
          "name": "label",
          "required": true,
          "type": "string"
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/downloads": {
    "post": {
      "consumes": ["multipart/form-data"],
      "parameters": [
        {
          "in": "body",
          "name": "_body",
          "required": true
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/filehistory/{node}/{path}": {
    "get": {
      "parameters": [
        {
          "in": "query",
          "name": "fields",
          "required": false,
          "type": "string"
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/hooks": {
    "post": {
      "parameters": [
        {
          "in": "body",
          "name": "_body",
          "required": true
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/issues/{issue_id}": {
    "put": {
      "parameters": [
        {
          "in": "body",
          "name": "_body",
          "required": true,
          "schema": {
            "$ref": "#/definitions/issue"
          }
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/issues/{issue_id}/attachments": {
    "post": {
      "consumes": ["multipart/form-data"],
      "parameters": [
        {
          "in": "body",
          "name": "_body",
          "required": true
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/pullrequests/activity": {
    "get": {
      "parameters": [
        {
          "name": "pull_request_id",
          "type": "integer",
          "deleted": true
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/refs/branches": {
    "post": {
      "parameters": [
        {
          "in": "body",
          "name": "_body",
          "required": true
        }
      ]
    }
  },
  "/repositories/{username}/{repo_slug}/src": {
    "post": {
      "consumes": ["multipart/form-data", "application/x-www-form-urlencoded"],
      "parameters": [
        {
          "in": "body",
          "name": "_body",
          "required": true
        },
        {
          "in": "body",
          "name": "author",
          "required": false,
          "type": "string"
        },
        {
          "in": "body",
          "name": "branch",
          "required": false,
          "type": "string"
        },
        {
          "in": "body",
          "name": "files",
          "required": false,
          "type": "string"
        },
        {
          "in": "body",
          "name": "message",
          "required": false,
          "type": "string"
        },
        {
          "in": "body",
          "name": "parents",
          "required": false,
          "type": "string"
        }
      ]
    }
  },
  "/snippets/{username}/{encoded_id}/watch": {
    "delete": {
      "responses": {
        "204": {
          "schema": {
            "$ref": ""
          }
        }
      }
    },
    "get": {
      "responses": {
        "204": {
          "schema": {
            "$ref": ""
          }
        }
      }
    },
    "put": {
      "responses": {
        "204": {
          "schema": {
            "$ref": ""
          }
        }
      }
    }
  },
  "/teams/{username}/members": {
    "get": {
      "deprecated": true
    }
  },
  "/users/{username}/followers": {
    "get": {
      "deprecated": true
    }
  },
  "/users/{username}/following": {
    "get": {
      "deprecated": true
    }
  },
  "/users/{username}/members": {
    "get": {
      "deprecated": true
    }
  }
}
